<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.maysah.krnfallex.krnfall.PageInfoEngActivity"
    android:background="@drawable/page3th1"
    android:id="@+id/imgbg">

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <TextView
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:id="@+id/namefallth"
            android:layout_marginTop="75dp"
            android:textColor="#ffffff"
            android:textSize="@dimen/H0"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="13dp"
            android:layout_marginEnd="10dp"
            android:breakStrategy="high_quality"
            android:hyphenationFrequency="full"/>


    </RelativeLayout>


    <ScrollView

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="45dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="60dp"
        android:layout_alignParentEnd="true"
        android:id="@+id/scrollView"
        android:theme="@style/BrandedCheckBox">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/layoutyoutube"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <fragment
                    android:name="com.google.android.youtube.player.YouTubePlayerFragment"
                    android:id="@+id/youtubeplayerfragment"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:visibility="visible" />
            </LinearLayout>

            <android.support.v4.view.ViewPager
                android:layout_width="264dp"
                android:layout_height="190dp"
                android:id="@+id/viewPager"
                android:layout_gravity="center_horizontal"
                android:visibility="gone" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/txtgeneralth"
                android:textColor="@color/material_grey_100"
                android:textSize="@dimen/h3"
                android:textIsSelectable="false"
                android:breakStrategy="high_quality"
                android:hyphenationFrequency="full"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/material_grey_100"
                android:textSize="@dimen/h0"
                android:text="@string/txt_null"
                android:id="@+id/textView7"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/txtfeeth"
                android:textColor="@color/material_grey_100"
                android:textSize="@dimen/h3"
                android:breakStrategy="high_quality"
                android:hyphenationFrequency="full"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/txtseason"
                android:textColor="@color/material_grey_100"
                android:textSize="@dimen/h0"
                android:textStyle="bold"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <CheckBox
                    android:text="@string/txt_rainseasoneng"
                    android:layout_width="77dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/checkBox3"
                    android:visibility="gone"
                    android:contextClickable="false"
                    android:button="@drawable/custom_checkbox"
                    android:clickable="false"
                    android:textColor="@color/common_google_signin_btn_text_dark_default"
                    android:textSize="16sp"
                    android:layout_weight="1.52"
                    android:paddingLeft="5dp"
                    android:layout_marginLeft="10dp"
                    android:breakStrategy="high_quality"
                    android:hyphenationFrequency="full"/>

                <CheckBox
                    android:text="@string/txt_coolseasoneng"
                    android:layout_width="98dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/checkBox2"
                    android:visibility="gone"
                    android:contextClickable="false"
                    android:clickable="false"
                    android:button="@drawable/custom_checkbox"
                    android:textColor="@color/common_google_signin_btn_text_dark_default"
                    android:textSize="16sp"
                    android:paddingLeft="5dp" />

                <CheckBox
                    android:text="@string/txt_hotseasoneng"
                    android:layout_width="79dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/checkBox1"
                    android:visibility="gone"
                    android:contextClickable="false"
                    android:button="@drawable/custom_checkbox"
                    android:clickable="false"
                    android:textColor="@color/common_google_signin_btn_text_dark_default"
                    android:textSize="16sp"
                    android:layout_weight="2.98"
                    android:paddingLeft="5dp" />
            </LinearLayout>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView8"
                android:textColor="@color/material_grey_100"
                android:textSize="@dimen/h0"
                android:textStyle="bold"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/texttravelth"
                android:textColor="@color/material_grey_100"
                android:textSize="@dimen/h3"
                android:breakStrategy="high_quality"
                android:hyphenationFrequency="full"/>

        </LinearLayout>
    </ScrollView>
    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton7"
        android:src="@drawable/icinfowhite"
        android:background="#00000000"
        android:layout_marginTop="5dp"
        android:layout_alignTop="@+id/scrollView"
        android:layout_gravity="left"
        android:layout_alignParentEnd="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentRight="true"
        android:onClick="titleview"
        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic360"
        android:id="@+id/imageButton9"
        android:onClick="clickvitualreality"
        android:layout_alignParentEnd="true"
        android:background="#00000000"
        android:layout_below="@+id/imageButton8"
        android:layout_alignLeft="@+id/imageButton6"
        android:layout_gravity="left"
        android:layout_marginTop="8dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icplay"
        android:id="@+id/imageButtoncallth"
        android:background="#00000000"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imageButton9"
        android:layout_alignLeft="@+id/imageButton6"
        android:layout_gravity="left"
        android:layout_marginTop="8dp"
        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icgps"
        android:id="@+id/imageButton6"
        android:background="#00000000"
        android:onClick="clickGpsFall"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btrtif"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="#00000000"
        android:onClick="btrtif" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icstar"
        android:id="@+id/imageButton8"
        android:background="#00000000"
        android:layout_gravity="left"
        android:onClick="historyth"
        android:layout_below="@+id/imageButton7"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="8dp"/>
</RelativeLayout>